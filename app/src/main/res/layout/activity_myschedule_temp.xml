<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_register">





        <TextView
        android:id="@+id/meetingList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="My Meetings"
        android:textColor="@color/white"
        android:textSize="30dp"
        android:textStyle="bold" />

        <EditText
            android:id="@+id/meeting1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/input_register_bg"
            android:hint="@string/hint_email"
            android:padding="10dp"
            android:singleLine="true"
            android:linksClickable="true"
            android:textColor="@color/input_register"
            android:text="enginnering meet"
            android:layout_below="@+id/meetingList"
            android:layout_alignParentStart="true"
            android:layout_marginTop="50dp" />

        <EditText
            android:id="@+id/meeting2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/input_register_bg"
            android:padding="10dp"
            android:singleLine="true"
            android:textColor="@color/input_register"
            android:linksClickable="true"
            android:text="Project Set Up Meeting"
            android:layout_below="@+id/meeting1"
            android:layout_alignParentStart="true" />


        <EditText
            android:id="@+id/meeting3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/input_register_bg"
            android:padding="10dp"
            android:singleLine="true"
            android:linksClickable="true"
            android:textColor="@color/input_register"
            android:text="Testing Setup"
            android:layout_below="@+id/meeting2"
            android:layout_alignParentStart="true" />





</RelativeLayout>
